{
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/books",
            "dest": "/api/v1/books"
        },
        {
            "src": "/api/v1/books/:id",
            "dest": "/api/v1/books/:id"
        },
        {
            "src": "/api/v1/books",
            "method": "post",
            "dest": "/api/v1/books"
        },
        {
            "src": "/api/v1/books/:id",
            "method": "put",
            "dest": "/api/v1/books/:id"
        },
        {
            "src": "/api/v1/books/:id",
            "method": "delete",
            "dest": "/api/v1/books/:id"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}